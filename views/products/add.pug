extends ../layout

block content
    h1 Add Your Feedback
    form(action="/products/add",method="post")
        .form-group
            label(for="name") Username
            input#name(type="text", class="form-control", name="name")
        .form-group
            label(for="service") Feedback
            input#contact(type="text", class="form-control", name="service")
        input(type="submit", class="btn btn-info")
        